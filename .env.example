# Frontend Environment Variables
# Copy this file to .env.local and update with your values

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_WS_URL=ws://localhost:3001/rt/messages

# BlackBerry Dynamics Configuration
VITE_GD_APP_ID=com.yourcompany.messaging
VITE_GD_APP_VERSION=1.0.0
VITE_GD_SERVER_URL=https://your-uem-server.com

# Feature Flags
VITE_ENABLE_QUANTUM_MODE=true
VITE_ENABLE_FILE_SHARING=true
VITE_ENABLE_GROUP_CHAT=true

# Debug Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
